<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.analyse.Controller.AppointmentNumberIKRController"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="@style1.css">

    <BarChart fx:id="barChart" title="Number of Appointments Per Month" layoutX="10.0" layoutY="10.0" prefHeight="380.0" prefWidth="580.0">
        <xAxis>
            <CategoryAxis fx:id="xAxis" />
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="yAxis" />
        </yAxis>
    </BarChart>
</AnchorPane>
