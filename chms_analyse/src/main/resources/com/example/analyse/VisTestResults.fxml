<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.analyse.Controller.Vis2TestResultController"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="@style1.css"> <!-- Link to CSS file -->
    <BarChart fx:id="barChart"
              layoutX="50.0" layoutY="50.0"
              prefHeight="300.0" prefWidth="500.0">
        <xAxis>
            <CategoryAxis fx:id="xAxis" label="Month"/>
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="yAxis" label="Number of Tests"/>
        </yAxis>
    </BarChart>
</AnchorPane>
